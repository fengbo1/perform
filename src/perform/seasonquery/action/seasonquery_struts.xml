		<action name="seasonkbidetail_query" class="perform.seasonquery.action.SeasonKBIDetailQuery">
			<result name="success">/page/seasonquery/seasonquerydetail_kbi.jsp</result>
		</action>
		<action name="seasonkcidetail_query" class="perform.seasonquery.action.SeasonKCIDetailQuery">
			<result name="success">/page/seasonquery/seasonquerydetail_kci.jsp</result>
		</action>
		<action name="exportpscore" class="perform.seasonquery.action.ExportPscore">
			<result name="success">/page/public/derivedetail.jsp</result>
		</action>
	</package>
